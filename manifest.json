{
    
    "manifest_version": 2,
    "name": "WhatsNearBy",
    "version": "1.0",
    "description": "Check what is nearby you.",
    "icons": {
        "48": "icons/48.png"
    },
    "permissions": [
        "activeTab",
        "geolocation"
    ],
    "applications": {
        "gecko": {
            "id": "hanny562@hotmail.com"
        }
    },
    "background": {
        "scripts": ["whatsnearby.js"]
    },
    "browser_action": {
        "default_icon": "icons/48.png",
        "default_title": "What's Nearby",
        "default_popup": "whatsnearby.html"
    }
    
}